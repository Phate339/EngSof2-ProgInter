                    <div class="item-slider owl-carousel owl-theme" data-toggle="owlcarousel" data-owlcarousel-settings="{&quot;singleItem&quot;:true, &quot;navigation&quot;:true, &quot;transitionStyle&quot;:&quot;fadeUp&quot;}" style="opacity: 1; display: block;">
